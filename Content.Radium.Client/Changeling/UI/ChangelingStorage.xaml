<controls1:FancyWindow xmlns="https://spacestation14.io"
                       xmlns:controls1="clr-namespace:Content.Client.UserInterface.Controls;assembly=Content.Client"
                      Title="{Loc 'tech-disk-ui-name'}"
                      SetSize="500 400">
    <BoxContainer Orientation="Vertical"
                  HorizontalExpand="True"
                  VerticalExpand="True"
                  Margin="10 10 10 10">
        <ItemList
            Name="IdentitiesList"
            HorizontalExpand="True"
            VerticalExpand="True">
        </ItemList>
        <Control MinHeight="10"/>
        <Button Name="ConfirmTransformation" Text="Подтвердить трансформацию" Disabled="True"></Button>
    </BoxContainer>
</controls1:FancyWindow>
